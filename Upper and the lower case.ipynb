    "def up_low(str):\n",
    "    u=0\n",
    "    l=0\n",
    "    for i in str:\n",
    "        if i.isupper():\n",
    "            u+=1\n",
    "        elif i.islower():\n",
    "            l+=1\n",
    "    print('Count of upper char',u)\n",
    "    print('Count of lower char',l)\n",
    "up_low('The quik Brow Fox')"
   ]
